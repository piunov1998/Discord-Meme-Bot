{
  "\u041b\u041a": {
    "author": "\u0410\u043d\u0442\u043e\u043d",
    "text": "\u0422\u044b \u0435\u0451 \u043d\u0435 \u0431\u044c\u0451\u0448\u044c - \u043f\u0438\u0437\u0434\u044b \u043f\u043e\u043b\u0443\u0447\u0430\u0435\u0448\u044c. \u0422\u044b \u0435\u0435 \u0431\u044c\u0451\u0448\u044c - \u0432\u0441\u0435 \u0440\u0430\u0432\u043d\u043e \u043f\u0438\u0437\u0434\u044b \u043f\u043e\u043b\u0443\u0447\u0430\u0435\u0448\u044c"
  },
  "\u0430\u043d\u0438\u043c\u0435": {
    "author": "\u0410\u043d\u0442\u043e\u043d",
    "text": "\u0423\u0431\u0435\u0440\u0438 \u044d\u0442\u043e \u0430\u043d\u0438\u043c\u0435 \u0441 \u0430\u0432\u044b \u0441\u0441\u0430\u043d\u043e\u0435, \u0437\u0430\u0435\u0431\u0430\u043b"
  },
  "\u0430\u043d\u0438\u043c\u04352": {
    "author": "\u0410\u043d\u0442\u043e\u043d",
    "text": "\u042f \u043d\u0435 \u0437\u043d\u0430\u044e \u0441\u043c\u043e\u0442\u0440\u0438\u0448\u044c \u0442\u044b \u0430\u043d\u0438\u043c\u0435 \u0438\u043b\u0438 \u043d\u0435\u0442, \u043d\u043e \u0443\u0432\u0435\u0440\u0435\u043d, \u0447\u0442\u043e \u0442\u044b \u0432 \u044d\u0442\u043e\u043c \u0437\u0430\u043c\u0435\u0448\u0430\u043d"
  },
  "\u043c\u0430\u0441\u0442\u0435\u0440": {
    "author": "\u0441\u0442\u0435\u0440",
    "text": "\u0440\u0435\u043a\u0442\u0430\u043b\u044c\u043d\u044b\u0445 \u0434\u0435\u043b \u043c\u0430"
  },
  "\u0440\u043e\u0437\u043e\u0432\u044b\u0435": {
    "author": "\u0410\u043d\u0442\u043e\u043d",
    "text": "\u041f\u043e\u0447\u0435\u043c\u0443 \u043c\u044b \u0440\u043e\u0437\u043e\u0432\u044b\u0435, \u0442\u044b \u0447\u0442\u043e \u0434\u0435\u0431\u0438\u043b?"
  },
  "\u0443\u0441\u043f\u0435\u043b": {
    "author": "\u0410\u043d\u0434\u0440\u0435\u0439",
    "text": "\u041f\u043e\u0447\u0442\u0438 \u0448\u0435\u043b, \u043f\u043e\u0447\u0442\u0438 \u0443\u0441\u043f\u0435\u043b."
  }
}